<div class="modal-header">
	<h3>{{ modalOptions.headerText }}</h3>
</div>
<div class="modal-body">
	<p>{{ modalOptions.bodyText }}</p>
</div>
<div class="modal-footer">
	<button type="button" class="btn" data-ng-click="modalOptions.close()">
		<%= t('modals.general.close') %>
	</button>
	<button class="btn btn-primary" data-ng-click="modalOptions.ok();">
		<%= t('modals.general.ok') %>
	</button>
</div>
